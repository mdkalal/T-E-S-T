@model TestDemo.Models.TrackOrderModel

@{
    ViewData["Title"] = "Track Order";
}

<h1>@ViewData["Title"]</h1>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

<div class="form-group">
    <div class="col-md-12">
        @Html.LabelFor(o => o.EmailAddress)
        @Html.EditorFor(o => o.EmailAddress)


    </div>
    <div class="col-md-12">
        @Html.LabelFor(o => o.OrderNumber)
        @Html.EditorFor(o => o.OrderNumber)

    </div>

    <div class="alert-danger">
        @Html.ValidationSummary(false)
    </div>


</div>


    <hr />
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10 text-center">
            <input type="submit" value="Check Order" class="btn btn-primary" />
        </div>
    </div>
       
}

